{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/((?!api|static|favicon.ico|.\\.(js|css|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot)).)",
      "dest": "/index.html"
    },
    {
      "src": "/(.*\\.(js|css|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot))",
      "dest": "/$1"
    }
  ],
  "rewrites": [
    {
      "source": "/path-in-repo1",
      "destination": "/path-in-repo2"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}

